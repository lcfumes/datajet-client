{
    "name": "dafiti/datajet-client",
    "description": "Dafiti Datajet Client for PHP",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "dafiti",
        "datajet",
        "client",
        "search"
    ],
    "authors": [
        {
            "name": "Fabio da Silva Ribeiro",
            "email": "fabio.ribeiro@dafiti.com.br"
        }
    ],
    "support": {
        "issue": "https://github.com/dafiti/datajet-client/issues"
    },
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "^6.1.0",
        "php": ">=5.5.0",
        "respect/validation": "^1.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "fabpot/php-cs-fixer": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Dafiti\\Datajet\\": "src/Datajet/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dafiti\\Datajet\\": "tests/Datajet/"
        }
    }
}
